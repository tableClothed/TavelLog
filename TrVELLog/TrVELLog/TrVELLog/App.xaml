<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TrVELLog.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="newColor">#0083A7</Color>
            <Color x:Key="textColor">#FFFFFF</Color>
            

            <Style TargetType="Button" x:Key="normalButton">
                
                <Setter Property="BackgroundColor" Value="{StaticResource textColor}"></Setter>
                <Setter Property="TextColor" Value="{StaticResource newColor}"></Setter>

            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>